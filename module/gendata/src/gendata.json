"name": "gendata",
"version": "1.1.2",
"use_mpi": false,
"use_cuda": false,
"dataname": {
  "type": "argument",
  "format": "text",
  "required": true,
  "repeatable": false,
  "default": "TRACE",
  "width": "default",
  "help": "Name of the attribute name of seismic data"
},
"maxtime": {
  "type": "argument",
  "format": "int",
  "required": true,
  "repetable": false,
  "default": 5000,
  "width": "default",
  "help": "The last time of the time axis in milliseconds"
},
"sinterval": {
  "type": "argument",
  "required": true,
  "repetable": false,
  "format": "int",
  "default": 4000,
  "width": "default",
  "help": "Sampling interval time in microseconds"
},
"primarykey": {
  "type": "procedure",
  "required": true,
  "repetable": false,
  "name": {
    "type": "argument",
    "required": true,
    "repetable": false,
    "format": "text",
    "width": "default",
    "help": "The name of the attribute of the primary key"
  },
  "first": {
    "type": "argument",
    "required": true,
    "repetable": false,
    "format": "int",
    "default": 1,
    "width": "default",
    "help": "The first primary key value"
  },
  "last": {
    "type": "argument",
    "required": true,
    "repetable": false,
    "format": "int",
    "default": 10,
    "width": "default",
    "help": "The last primary key value"
  },
  "step": {
    "type": "argument",
    "required": true,
    "repetable": false,
    "format": "int",
    "default": 1,
    "width": "default",
    "help": "The step size of the primary key values"
  }
},
"secondarykey": {
  "type": "procedure",
  "required": true,
  "repetable": false,
  "name": {
    "type": "argument",
    "required": true,
    "repetable": false,
    "format": "text",
    "width": "default",
    "help": "The name of the attribute of the secondary key"
  },
  "first": {
    "type": "argument",
    "required": true,
    "repetable": false,
    "format": "int",
    "default": 1,
    "width": "default",
    "help": "The first secondary key value"
  },
  "last": {
    "type": "argument",
    "required": true,
    "repetable": false,
    "format": "int",
    "default": 100,
    "width": "default",
    "help": "The last secondary key value"
  },
  "step": {
    "type": "argument",
    "required": true,
    "repetable": false,
    "format": "int",
    "default": 1,
    "width": "default",
    "help": "The step size of the secondary key value"
  }
},
"signal": {
  "required": true,
  "repeatable": true,
  "type": "option",
  "help": "Definition of signal to be generated and added to the data",
  "options": [
    {
    "ormsby": {
      "type": "procedure",
      "required": true,
      "repetable": false,
      "help": "Ormsby wavelet",
      "f1": {
        "type": "argument",
        "required": true,
        "repetable": false,
        "help": "The low-cut frequency in Hz",
        "formt": "float",
        "default": 5.0
      },
      "f2": {
        "type": "argument",
        "required": true,
        "repetable": false,
        "help": "The low-pass frequency in Hz",
        "formt": "float",
        "default": 10.0
      },
      "f3": {
        "type": "argument",
        "required": true,
        "repetable": false,
        "help": "The high-pass frequency in Hz",
        "formt": "float",
        "default": 40.0
      },
      "f4": {
        "type": "argument",
        "required": true,
        "repetable": false,
        "help": "The high-cut frequency in Hz",
        "formt": "float",
        "default": 45.0
      },
      "gate": {
        "type": "argument",
        "required": true,
        "repetable": false,
        "help": "The time window gate in ms",
        "formt": "int",
        "default": 500
      },
      "times": {
        "type": "argument",
        "required": true,
        "repetable": true,
        "help": "The center time value of the gate in ms",
        "formt": "int",
        "default": 500
      }
    }
    },
    {
    "ricker": {
      "type": "procedure",
      "required": true,
      "repetable": false,
      "help": "Ricker wavelet",
      "pfreq": {
        "type": "argument",
        "required": true,
        "repetable": false,
        "help": "The peak frequency in Hz",
        "formt": "float",
        "default": 15.0
      },
      "gate": {
        "type": "argument",
        "required": true,
        "repetable": false,
        "help": "The time window gate in ms",
        "formt": "int",
        "default": 500
      },
      "times": {
        "type": "argument",
        "required": true,
        "repetable": true,
        "help": "The time value in ms where to add the signal",
        "formt": "int",
        "default": 500
      }
    }
    }
  ]
}
