"name": "segyoutput",
"version": "1.0.0",
"use_mpi": false,
"use_cuda": false,
"output_url": {
  "type": "argument",
  "required": true,
  "format": "text",
  "default": "",
  "help": "The full path where the SEGY output file will be created, ie, /full/path/to/the/output_data.segy",
  "width": "full",
  "button": "save-file",
  "filefilter": "*.segy"
},
"primary_offset":{
  "type": "argument",
  "required": false,
  "help": "the offset of primary number in trace headers (default: 189 for inline)",
  "format": "int",
  "default": "189",
  "width": "default"
},
"secondary_offset":{
  "type": "argument",
  "required": false,
  "help": "the offset of crossline number in trace headers (default: 193 for crossline)",
  "format": "int",
  "default": "193",
  "width": "default"
},
"sinterval_offset":{
  "type": "argument",
  "required": false,
  "help": "the offset of sample interval in trace headers (default: 17)",
  "format": "int",
  "default": "17",
  "width": "default"
},
"trace_length_offset":{
  "type": "argument",
  "required": false,
  "help": "the offset of trace length in trace headers (default: 21)",
  "format": "int",
  "default": "21",
  "width": "default"
},
"data_format_code_offset":{
  "type": "argument",
  "required": false,
  "help": "the offset of data format code in trace headers (default: 25)",
  "format": "int",
  "default": "25",
  "width": "default"
},
"textual_header": {
  "type": "argument",
  "required": false,
  "format": "text",
  "width": "full",
  "help": "Custom textual header content (3200 characters max). If not provided, automatic header will be generated."
},
"textual_header":{
  "type": "argument",
  "required": false,
  "help": "The user-defined section of the textual header will be put into the textual header.",
  "format": "text",
  "width": "full"
},
"attribute": {
  "required": false,
  "repeatable": true,
  "type": "procedure",
  "help": "Custom attributes to write to SEGY trace headers",
  "name": {
    "type": "argument",
    "required": true,
    "repetable": false,
    "format": "text",
    "width": "default",
    "help": "Name of the attribute"
  },
  "datatype": {
  "type": "choice",
  "required": true,
  "help": "The data type of the current attribute: int8, int16, int32 and float.",
  "format": "text",
  "default": "int32",
  "options": [
    "int8",
    "int16",
    "int32",
    "float"
  ]
  },
  "offset": {
    "type": "argument",
    "required": true,
    "help": "the offset of attribute in trace headers (240 bytes)",
    "format": "int"
  }
}